<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.shahwar.cafecoffeesquare">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CoffeeList"
            android:label="@string/title_activity_coffee_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SnacksList"
            android:label="@string/title_activity_snacks_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ItemDesc"
            android:label="@string/title_activity_snacks_desc"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CakesList"
            android:label="@string/title_activity_cakes_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".FoodList"
            android:label="@string/title_activity_food_list"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>